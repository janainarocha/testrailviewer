{
  "name": "testrailviewer",
  "version": "1.0.0",
  "description": "A simple web viewer for TestRail test cases, created to allow people without direct access to TestRail to view specific test cases.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t testrailviewer .",
    "docker:run": "docker run -p 3000:3000 --env-file .env testrailviewer",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "test": "echo 'Visit http://localhost:3000/?case=381377 to test'"
  },
  "keywords": [
    "testrail",
    "viewer",
    "docker",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.3",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
